{
  "ApiSettings": {
    "GatewayAddress": "https://localhost:5010"

  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning",
        "Elastic.Apm": "Information"
      }
    }
  },
  "ElasticApm": {

